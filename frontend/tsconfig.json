{
    "compilerOptions": {
        "types": ["node"], 
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"],
        },
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Node",
        "strict": true,
        "jsx": "react-jsx",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,   
        "skipLibCheck": true,
    },
    "include": ["src"]
}
